{
  "name": "@ventus/server",
  "version": "1.0.0",
  "main": "./src/main.ts",
  "scripts": {
    "start": "export COMMIT_SHA=$(git rev-parse --short HEAD) NODE_ENV=production && ts-node ./src/main.ts",
    "dev": "export COMMIT_SHA=$(git rev-parse --short HEAD) NODE_ENV=development && nodemon ./src/main.ts | pino-pretty",
    "start:watch": "export COMMIT_SHA=$(git rev-parse --short HEAD) NODE_ENV=development && nodemon ./src/main.ts | pino-pretty",
    "start:prod": "export COMMIT_SHA=$(git rev-parse --short HEAD) NODE_ENV=production && node ./build",
    "build": "rm -rf build && pnpm db:generate && tsc",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:generate": "prisma generate",
    "clean": "rm -rf node_modules build",
    "format": "prettier --write **/*"
  },
  "author": "Aelpxy",
  "license": "MIT",
  "dependencies": {
    "@fastify/auth": "^4.4.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/etag": "^5.1.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "^5.8.0",
    "argon2": "^0.31.2",
    "axios": "^1.6.5",
    "bullmq": "^5.1.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "fastify-raw-body": "^4.3.0",
    "fastify-yup-schema": "^1.0.4",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "time2fa": "^1.3.0",
    "validator": "^13.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "@types/validator": "^13.11.8",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.1.1",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
